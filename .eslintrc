{
  "ignorePatterns": ["hosted/*"],
  "env": { "browser": true },
  "extends": ["airbnb/base", "plugin:react/recommended"],
  "rules": {
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "react/prop-types": "off"
  }
}
